                  <label for="op3DL" class="form-label">DRIVERS LICENSE NO.</label>
                  <input type="text" class="form-control" id="op3DL" />
                </div>
                {{! DOB }}
                <div class="mb-2">
                  <label for="op3DOB" class="form-label">DATE OF BIRTH</label>
                  <input id="op3DOB" class="form-control" type="text" />
                </div>
              </div>
            </div>
            <h3>OPERATOR #4 ( IF APPLICABLE )</h3>
            <div class="d-flex flex-column justify-content-center">
              {{! Name }}
              <div class="mb-2 mx-3">
                <label for="op4Name" class="form-label">NAME</label>
                <input type="text" class="form-control" id="op4Name" />
              </div>
              <div class="d-flex justify-content-around">
                {{! DL }}
                <div class="mb-2 mx-3">
                  <label for="op4DL" class="form-label">DRIVERS LICENSE NO.</label>
                  <input type="text" class="form-control" id="op4DL" />
                </div>
                {{! DOB }}
                <div class="mb-2">
                  <label for="op4DOB" class="form-label">DATE OF BIRTH</label>
                  <input id="op4DOB" class="form-control" type="text" />
                </div>
              </div>
            </div>
          </div>

        </div>
        <h2>ADDITIONAL INFORMATION</h2>
        <div class="d-flex flex-column justify-content-around">
          <div class="d-flex justify-content-evenly flex-wrap">
            <div class="d-flex flex-column">
              {{! Name }}
              <div class="mb-2 mx-3">
                <label for="name" class="form-label required">NAME</label>
                <div class="d-flex">
                  <div class="d-flex flex-column w-50 mx-3">
                    <input
                      type="text"
                      class="form-control"
                      id="firstName"
                      aria-describedby="firstHelp"
                      required
                    />
                    <div id="firstHelp" class="form-text">FIRST</div>
                  </div>
                  <div class="d-flex flex-column w-50 mx-3">
                    <input
                      type="text"
                      class="form-control"
                      id="lastName"
                      aria-describedby="lastHelp"
                      required
                    />
                    <div id="firstHelp" class="form-text">LAST</div>
                  </div>
                </div>
              </div>
              {{! Address}}
              <div class="mb-2 mx-3">
                <label
                  for="address1"
                  class="form-label required"
                >ADDRESS</label>
                <input
                  type="text"
                  class="form-control"
                  id="address1"
                  aria-describedby="adress1Help"
                  required
                />
                <div id="address1Help" class="form-text mb-2">LINE 1</div>
                <input
                  type="text"
                  class="form-control"
                  id="address2"
                  aria-describedby="adress2Help"
                />
                <div id="address2Help" class="form-text mb-2">LINE 2</div>
                <div class="d-flex mb-2">
                  <div class="d-flex flex-column w-50 mx-3">
                    <input
                      type="text"
                      class="form-control"
                      id="city"
                      aria-describedby="cityHelp"
                      required
                    />
                    <div id="cityHelp" class="form-text">CITY</div>
                  </div>
                  <div class="d-flex flex-column w-50 mx-3">
                    <input
                      type="text"
                      class="form-control"
                      id="state"
                      aria-describedby="stateHelp"
                      required
                    />
                    <div id="stateHelp" class="form-text">STATE</div>
                  </div>
                </div>
                <div class="d-flex mb-2">
                  <div class="d-flex flex-column w-50 mx-3">
                    <input
                      type="text"
                      class="form-control"
                      id="zip"
                      aria-describedby="zipHelp"
                      required
                    />
                    <div id="cityHelp" class="form-text">ZIP CODE</div>
                  </div>
                  <div class="d-flex flex-column w-50 mx-3">
                    <input
                      type="text"
                      class="form-control"
                      id="country"
                      aria-describedby="countryHelp"
                      required
                    />
                    <div id="countryHelp" class="form-text">COUNTRY</div>
                  </div>
                </div>
              </div>
              {{! EMAIL }}
              <div class="mb-2">
                <label for="inputEmail" class="form-label required">EMAIL
                  ADDRESS</label>
                <input
                  type="email"
                  class="form-control"
                  id="inputEmail"
                  aria-describedby="emailHelp"
                  required
                /><div id="emailHelp" class="form-text">We will never share your
                  email with anyone without consent.</div>
              </div>
              {{! PHONE }}
              <div class="mb-2">
                <label for="inputPhone" class="form-label">PHONE</label>
                <input
                  type="tel"
                  class="form-control mb-2"
                  id="inputPhone"
                  aria-describedby="phoneHelp"
                />
              </div>
            </div>
            <div class="d-flex flex-column">
              <div class="mb-2">
                <label for="prevCarrier" class="form-label required">PREVIOUS
                  CARRIER</label>
                <input
                  type="text"
                  class="form-control mb-2"
                  id="prevCarrier"
                  aria-describedby="prevCarrierHelp"
                  required
                />

                <div class="d-flex mb-2 justify-content-between">
                  {{! CONTINUOUS COVERAGE}}
                  <div class="mb-2">
                    <label
                      for="continuousCoverage"
                      class="form-label required"
                    >CONTINUOUS COVERAGE</label>
                    <select
                      id="continuousCoverage"
                      class="form-select form-control"
                      aria-label="Default select example"
                    >
                      <option selected></option>
                      <option value="3+ YEARS">3+ YEARS</option>
                      <option value="2 YEARS">2 YEARS</option>
                      <option value="1 YEAR">1 YEAR</option>
                      <option value="6 MONTHS">6 MONTHS</option>
                      <option value="UNDER 6 MONTHS">UNDER 6 MONTHS</option>
                      <option value="NOT CURRENTLY INSURED">NOT CURRENTLY
                        INSURED</option>
                    </select>
                  </div>
                  {{! POLICY EXPIRATION}}
                  <div class="mb-2">
                    <label for="policyExp" class="form-label required">POLICY
                      EXPIRES IN</label>
                    <select
                      id="policyExp"
                      class="form-select form-control"
                      aria-label="Default select example"
                    >
                      <option selected></option>
                      <option value="NOT SURE">NOT SURE</option>
                      <option value="A FEW DAYS">A FEW DAYS</option>
                      <option value="2 WEEKS">2 WEEKS</option>
                      <option value="1 MONTH">1 MONTH</option>
                      <option value="2 MONTHS">2 MONTHS</option>
                      <option value="3-6 MONTHS">3-6 MONTHS</option>
                      <option value="6+ MONTHS">6+ MONTHS</option>
                      <option value="NOT CURRENTLY INSURED">NOT CURRENTLY
                        INSURED</option>
                    </select>
                  </div>
                </div>
                <div class="d-flex mb-2 justify-content-between">
                  {{! CLAIMS IN 3 YEARS}}
                  <div class="mb-2">
                    <label for="claimsIn3Yr" class="form-label required">CLAIMS
                      IN 3 YEARS</label>
                    <select
                      id="claimsIn3Yr"
                      class="form-select form-control"
                      aria-label="Default select example"
                    >
                      <option selected></option>
                      <option value="0">0</option>
                      <option value="1">1</option>
                      <option value="2">2</option>
                      <option value="3">3</option>
                      <option value="4+">4+</option>
                    </select>
                  </div>
                  {{! TICKETS IN 3 YEARS}}
                  <div class="mb-2">
                    <label
                      for="ticketsIn3Yr"
                      class="form-label required"
                    >TICKETS IN 3 YEARS</label>
                    <select
                      id="ticketsIn3Yr"
                      class="form-control form-select"
                      aria-label="Default select example"
                    >
                      <option selected></option>
                      <option value="0">0</option>
                      <option value="1">1</option>
                      <option value="2">2</option>
                      <option value="3">3</option>
                      <option value="4">4</option>
                      <option value="5">5</option>
                      <option value="6+">6+</option>
                    </select>
                  </div>
                </div>
                {{! COVERAGE DESIRED}}
                <div class="mb-2">
                  <label
                    for="coverageDesired"
                    class="form-label required"
                  >COVERAGE DESIRED</label>
                  <select
                    id="coverageDesired"
                    class="form-select form-control"
                    aria-label="Default select example"
                  >
                    <option selected></option>
                    <option value="STANDARD COVERAGE">STANDARD COVERAGE</option>
                    <option value="PREMIUM COVERAGE">PREMIUM COVERAGE</option>
                    <option value="STATE MINIMUM">STATE MINIMUM</option>
                  </select>
                </div>
              </div>
              {{! MESSAGE BODY }}
              <div class="mb-2">
                <label for="inputMessage" class="form-label">Message</label>
                <textarea
                  id="inputMessage"
                  name="inquiry"
                  cols="40"
                  rows="5"
                  class="form-control"
                  required
                ></textarea>
              </div>
              {{! PREFERRED CONTACT METHOD }}
              <div class="mb-2">
                <label class="form-label required">How would you like us to
                  contact you?</label>
                {{! Email Option }}
                <div class="form-check">
                  <input
                    class="form-check-input"
                    type="radio"
                    name="flexRadio"
                    value="email"
                    id="emailSelect"
                    checked
                  />
                  <label class="form-check-label" for="flexRadioDefault1">
                    Email
                  </label>
                </div>
                {{! Phone Option }}
                <div class="form-check">
                  <input
                    class="form-check-input"
                    type="radio"
                    name="flexRadio"
                    value="phone"
                    id="phoneSelect"
                    required
                  />
                  <label class="form-check-label" for="flexRadioDefault2">
                    Phone
                  </label>
                </div>
              </div>

            </div>
          </div>

        </div>
        <button
          id="autoQuoteSubmitBtn"
          type="submit"
          class="btn btn-danger"
        >Submit</button>
      </div>
      <p id="submitMessage" style="display:none;">Your request for a quote has
        been successfully submitted. We'll get back to you ASAP.</p>
    </form>
  </div>

  <script src="/js/quotes/autos/autoQuote.js"></script>
</main>